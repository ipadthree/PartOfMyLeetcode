//I did this by myself

class Solution {
public:
    int uniquePaths(int m, int n) {
        if(m == 0 || n == 0) return 0;
        if(m == 1 || n == 1) return 1;
        vector<vector<int>> grid(m, vector<int>(n,1));
        grid[0][0] = 0;
        for(int i = 1; i < n; i++){
            for(int j = 1; j < m ; j++){
                grid[j][i] = grid[j][i-1] + grid[j-1][i];
            }
        }
        return grid[m-1][n-1];
    }
};
